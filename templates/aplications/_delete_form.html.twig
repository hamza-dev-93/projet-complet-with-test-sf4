<form method="post" action="{{ path('aplications_delete', {'id': aplication.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ aplication.id) }}">
    <button class="btn">Delete</button>
</form>
